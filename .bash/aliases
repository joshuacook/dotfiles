function doxer {
    docker run -it --rm -v `pwd`:/home/jovyan jupyter/datascience-notebook pandoc $1.md -o $1.docx
}

function nbconverter {
    docker run -it -v `pwd`:/home/jovyan nbconvert python /home/convert.py /home/jovyan/$1
}

function pdfer {
    docker run -it -v `pwd`:/home/$1 writing \ 
            pandoc \
            --template=mytemplate.latex \
            -F pandoc-crossref \
            -F pandoc-citeproc \
            --listings \
            -F /home/pandoc-wrapfig.py \
            --bibliography /home/bib.bib \
            /home/$1.md -o /home/$1.pdf
}

function beamer {
    docker run -it --rm -v `pwd`:/home/jovyan jupyter/datascience-notebook pandoc -o $1.pdf -t beamer $1.md
}
