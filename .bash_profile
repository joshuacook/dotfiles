export HOSTNAME="LOCAL"
export HISTFILESIZE=
export EDITOR=vim

 alias C='clear'
 alias c='cd'
 alias d='docker'
 alias dc='docker-compose'
 alias de='docker exec -it'
 alias G="git add -A && git commit -m 'latest' && git push"
 alias l="clear && ls -l"
 alias L='clear && ls -la'
 alias P='vi ~/.bash_profile ~/.vimrc && source ~/.bash_profile'
 alias v='vim'
